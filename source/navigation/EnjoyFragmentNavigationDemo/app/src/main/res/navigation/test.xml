<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/test"
    app:startDestination="@id/oneFragment5">


    <fragment
        android:id="@+id/oneFragment5"
        android:name="com.example.xiangxue.enjoyfragmentnavigationdemo.fragment.OneFragment"
        android:label="fragment_1"
        tools:layout="@layout/fragment_1" >
        <action
            android:id="@+id/action_oneFragment5_to_twoFragment"
            app:destination="@id/twoFragment" />
    </fragment>
    <fragment
        android:id="@+id/twoFragment"
        android:name="com.example.xiangxue.enjoyfragmentnavigationdemo.fragment.TwoFragment"
        android:label="fragment_2"
        tools:layout="@layout/fragment_2" >
        <action
            android:id="@+id/action_twoFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_twoFragment_to_navigation_1"
            app:destination="@id/navigation_1" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.xiangxue.enjoyfragmentnavigationdemo.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <include app:graph="@navigation/navigation_1" />
</navigation>